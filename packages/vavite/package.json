{
	"name": "vavite",
	"version": "1.0.0-beta.8",
	"type": "module",
	"module": "./dist/index.js",
	"files": [
		"dist",
		"*.d.ts"
	],
	"main": "./dist/index.cjs",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./middleware": {
			"import": "./dist/middleware.js",
			"require": "./dist/middleware.cjs"
		},
		"./no-sirv": {
			"import": "./dist/no-sirv.js",
			"require": "./dist/no-sirv.cjs"
		},
		"./entry": {
			"import": "./dist/entry.js",
			"require": "./dist/entry.cjs"
		},
		"./manifest": "./dist/stub.cjs",
		"./ssr-manifest": "./dist/stub.cjs",
		"./html": "./dist/stub.cjs"
	},
	"description": "Cloud agnostic SSR with Vite",
	"author": "Fatih Ayg√ºn <cyco130@gmail.com>",
	"repository": "https://github.com/cyco130/vavite",
	"license": "MIT",
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepack": "rm -rf dist && pnpm build",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint:ts": "eslint . --max-warnings 0 --ignore-pattern dist"
	},
	"peerDependencies": {
		"vite": "^2.8.1"
	},
	"devDependencies": {
		"@cyco130/eslint-config": "^1.0.0",
		"@types/node": "^17.0.10",
		"eslint": "^8.7.0",
		"sirv": "^2.0.2",
		"tsup": "^5.11.11",
		"typescript": "^4.5.5",
		"vite": "^2.8.1"
	}
}
